<script>
    var Is_PageLoad = "0";

    $(function () {
        $('body').on('click', '#btnshow', function () {
            Is_PageLoad = "1";
            gridList.Refresh();
        });

        $('#ddlAppIds').on('click', function () {
            var selectedValue = "1"; // XLSX export value
            var desg = $("#drpdesignation").val();
            var usrtype = $("#ddlusertypes").val();
            var url = '@Url.Action("ExportFormTypeList", "FormType", new { type = "_type_" })';
            window.location.href = url.replace("_type_", selectedValue);
        });
    });

    function CancelAdd() {
        if ($("#hdnAdd").val() == "1") {
            Swal.fire({
                title: 'Are you sure you want to Close with out save data.?',
                text: "This action cannot be undone!",
                imageUrl: '/assests/images/delete-big.png', // Custom icon for confirmation
                imageWidth: 40, // Adjust the width as needed
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Yes',
                cancelButtonText: 'No'
            }).then((result) => {
                if (result.isConfirmed) {
                    $("#FormTypeModal").modal('hide');
                }
            });
        }

        else {
            $("#FormTypeModal").modal('hide');
        }

    }
    function OnStartCallback(s, e) {
        e.customArgs["Is_PageLoad"] = Is_PageLoad;
        document.getElementById("loading-overlay").style.display = "block";
    }

    function OnEndCallback() {
        document.getElementById("loading-overlay").style.display = "none";
    }

    function AddFormType() {
        $("#hdnFormTypeID").val('0');
        $("#txtFormTypeSurveyDesc").val('');
        $("#txtFormTypeRemarks").val('');
        $("#ddlFormTypeSecGroup").val(0);
        $('#IsActive').prop('checked', true);

        $("#FormTypeModal").modal('show')
        $("#hdnAdd").val('1');
    }


    function SaveFormType() {
        var id = $("#hdnFormTypeID").val();

        if ($("#txtFormTypeSurveyDesc").val().trim() == "") {
            // SweetAlert for empty category name
            Swal.fire({
                icon: 'warning',
                title: 'Warning',
                text: 'Please enter Survey Type to proceed.'
            });
            return false;
        }
        else {

            $.ajax({
                type: "POST",
                url: '@Url.Action("SaveFormType", "FormType")',
                data: {
                    SurveyDesc: $("#txtFormTypeSurveyDesc").val().trim(),
                    Remarks: $("#txtFormTypeRemarks").val().trim(),
                    SecGroup: $("#ddlFormTypeSecGroup").val(),
                    IsActive: $('#IsActive').prop('checked'),
                    FormTypeId: id
                },
                success: function (response) {
                    if (parseInt(response.Value) > 0) {
                        // SweetAlert success message
                        Swal.fire({
                            icon: 'success',
                            title: 'Success',
                            text: 'Form Type saved successfully.'
                        }).then(() => {
                            $("#FormTypeModal").modal('hide');
                            //location.reload();
                            Is_PageLoad = "1";
                            gridList.Refresh();
                        });
                    } else if (parseInt(response.Value) == -1) {
                        // SweetAlert duplicate category name warning
                        Swal.fire({
                            icon: 'error',
                            title: 'Sorry!',
                            text: 'Duplicate Form Type name cannot be entered. Please enter a unique category name.'
                        });
                    }
                }
            });
        }
    }


     function EditClick(ID) {
         $("#hdnFormTypeID").val(ID);
         $("#hdnAdd").val('0');
            $.ajax({
                type: "POST",
                url: "@Url.Action("EditFormType", "FormType")",
                data: { id: ID },
                success: function (response) {
                    if (response.code == "") {
                        jAlert('No data found');
                    }
                    else {
                        $("#hdnFormTypeID").val(ID);

                        $("#txtFormTypeSurveyDesc").val(response.FORMTYPE_SURVEYDESC);
                        $("#txtFormTypeRemarks").val(response.FORMTYPE_REMARKS);
                        $("#ddlFormTypeSecGroup").val(response.FORMTYPE_SECGROUPID);
                        $('#IsActive').prop('checked', response.FORMTYPE_ISACTIVE);

                        $("#FormTypeModal").modal('show');
                        $("#hdnAdd").val('2');

                        $("#modaltitle").text("Edit Form Type");
                        $("#FormTypeModal").modal('show');
                    }
                }
            });
        }

     function DeleteClick(ID) {
        $("#hdnFormTypeID").val(ID);

        // SweetAlert confirmation with custom icon
        Swal.fire({
            title: 'Are you sure you want to delete?',
            text: "This action cannot be undone!",
            imageUrl: '/assests/images/delete-big.png', // Custom icon for confirmation
            imageWidth: 40, // Adjust the width as needed
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Yes',
            cancelButtonText: 'No'
        }).then((result) => {
        if (result.isConfirmed) {
            $.ajax({
                type: "POST",
                url: '@Url.Action("Delete", "FormType")',
                data: { ID: ID },
                success: function (response) {
                    if (parseInt(response) > 0) {
                        // SweetAlert success message with custom icon
                        Swal.fire({
                            title: 'Deleted!',
                            text: 'The record has been deleted successfully.',
                            /*imageUrl: '/path/to/your/success-icon.png', */
                            /*imageWidth: 50, */
                            icon: 'success' // You can also keep the built-in icon type
                        }).then(() => {
                            //location.reload();
                            Is_PageLoad = "1";
                            gridList.Refresh();
                        });
                    } else if (parseInt(response) == -1) {
                        // SweetAlert error message with custom icon
                        Swal.fire({
                            title: 'Sorry!',
                            text: 'Form Type already mapped with another module. Cannot delete.',
                            imageUrl: '/path/to/your/error-icon.png',
                            imageWidth: 50,
                            icon: 'error' // You can also keep the built-in icon type
                        });
                    }
                }
            });
        }
    });
    }

