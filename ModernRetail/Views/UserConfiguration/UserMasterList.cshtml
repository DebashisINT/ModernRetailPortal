    function myFunction() {
        // Get the search input value
        const filter = document.getElementById("myInput").value.toLowerCase();

        // Get the list of all checkbox labels inside #divModalBody
        const items = document.querySelectorAll("#divModalBody .custom-checkbox-single");

        // Loop through all items in the list
        items.forEach(item => {
            // Get the text content of the label
            const text = item.textContent || item.innerText;

            // Show or hide the item based on whether it matches the filter
            if (text.toLowerCase().includes(filter)) {
                item.style.display = ""; // Show
            } else {
                item.style.display = "none"; // Hide
            }
        });
    }


    function myFunctionBranchMap() {
