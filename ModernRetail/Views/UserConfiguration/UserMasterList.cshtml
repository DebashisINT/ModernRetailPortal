    function StateBind(user_id) {
        $("#hdnID").val(user_id);
        var str
        str = { user_id: user_id }
        var html = "";
        // alert();
        $.ajax({
            type: "POST",
            url: "@Url.Action("GetStateList", "UserConfiguration")",
            data: JSON.stringify(str),
            contentType: "application/json; charset=utf-8",
            datatype: "json",
            success: function (responseFromServer) {
                if (responseFromServer.length == 0) {
                    Swal.fire({
                        icon: 'info',
                        title: 'No States Found',
                        text: 'You must create a user, and map this employee. After mapping, you can map the State. So, these employees will appear on Dashboard and in reports for the selected state.',
                        confirmButtonText: 'OK'
                    });
                } else if (responseFromServer[0].status != 'Success') {
                    Swal.fire({
                        icon: 'error',
                        title: 'Error',
                        text: 'You must create a user, and map this employee. After mapping, you can map the State. So, these employees will appear on Dashboard and in reports for the selected state.',
                        confirmButtonText: 'OK'
                    });
                } else {
                    let html = ""; // Initialize HTML variable
                    for (i = 0; i < responseFromServer.length; i++) {
                        const isChecked = responseFromServer[i].IsChecked ? "checked" : "";

                        if (responseFromServer[i].StateID == "0") {
                            html += `
                                <label class='custom-checkbox-single'>
                                    ${responseFromServer[i].StateName}
                                    <input type='checkbox' id='${responseFromServer[i].StateID}' class='statecheckall' onclick='CheckAll(${responseFromServer[i].StateID})' value='${responseFromServer[i].StateID}' ${isChecked}>
                                    <span class='checkmark'></span>
                                </label>`;
                        } else {
                            html += `
                                <label class='custom-checkbox-single'>
                                    ${responseFromServer[i].StateName}
                                    <input type='checkbox' id='${responseFromServer[i].StateID}' class='statecheck' onclick='CheckParticular($(this).is(":checked"))' value='${responseFromServer[i].StateID}' ${isChecked}>
                                    <span class='checkmark'></span>
                                </label>`;
                        }
                    }
                    $("#divModalBody").html(html);
                    $("#myModal").modal('show');
                }
            }
        });


    }

